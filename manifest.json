{
  "manifest_version": 2,
  "name": "FastAudioWhatsapp",
  "version": "1.0",

  "description": "Change the speed of audios in whatsapp web",

  "icons": {
    "48": "icons/icon_x48.png",
    "96": "icons/icon_x96.png"
  },

  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/"],
      "js": ["index.js"],
      "css": ["style.css"]
    }
  ]
}
